{
	"name": "@graphprotocol/query-examples-sveltekit",
	"description": "Demos how to query the Graph Network Arbitrum Subgraph published to The Graph Network using an API Key obtained on The Graph Studio in a SvelteKit application",
	"repository": {
		"url": "https://github.com/graphprotocol/query-examples",
		"directory": "/examples/sveltejs"
	},
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"codegen.gql": "graphql-codegen --config ./graphql-codegen.ts && bun prettier --write \"src/generated/**/*\"",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^5.0.2",
		"@graphql-codegen/client-preset": "^4.2.5",
		"@sveltejs/adapter-auto": "^3.2.0",
		"@sveltejs/kit": "^2.5.5",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/eslint": "^8.56.6",
		"@typescript-eslint/eslint-plugin": "^7.4.0",
		"@typescript-eslint/parser": "^7.4.0",
		"autoprefixer": "^10.4.19",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.38",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.8",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.3",
		"vite": "^5.2.6"
	},
	"type": "module",
	"dependencies": {
		"dotenv": "^16.4.5",
		"graphql": "^16.8.1",
		"graphql-request": "^6.1.0"
	}
}
